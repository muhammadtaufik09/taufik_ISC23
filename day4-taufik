package varible;

public class Varible {

    public static void main(String[] args) {
      //  deklarasi variable
      String nama;
      char inisial;
      int umur;
      float tb;
      double bb;
      boolean tampan;
      
       //   pemberian nilai ke variable      
       nama ="Muhammad Taufik";
       inisial = 'M';
       umur = 19;
       tb = 184.9f;
       bb = 73;
       tampan = true;
       
       
        System.out.println("NAMA ANDA:"+ nama);
        System.out.println("INISIAL NAMA ANDA:"+ inisial);
        System.out.println("UMUR ANDA:"+ umur);
        System.out.println("TINGGI BADAN ANDA:"+ tb);
        System.out.println("BERAT BADAN ANDA:"+bb);
        System.out.println("ANDA TAMPAN:"+ tampan);
     }
    
}
